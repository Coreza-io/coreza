{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "python": "${workspaceFolder}/graphin_backend/venv/Scripts/python.exe",
      "module": "uvicorn",
      "args": [
        "graphin_backend.main:app",
        "--reload"
      ],
      "jinja": true,
      "envFile": "${workspaceFolder}/graphin_backend/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/graphin_backend"
      }
    }
  ]
}
