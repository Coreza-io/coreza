{
  "name": "FinnHub",
  "def": "Fetch Market Data",
  "node_type": "main",
  "category": "Broker",
  "auth": "GenericAuthModal",
  "authFields": [     
    {
      "key": "credential_name",
      "label": "Credential Label",
      "type": "text",
      "default": "Personal FinnHub"
    },
    {
      "key": "api_key",
      "label": "FinnHub API Key",
      "type": "password",
      "placeholder": "API Secret key"
    }

  ],
  "authAction": {"url": "/finnhub/auth-url", "method": "POST"},
  "icon": "/assets/icons/FinnHub.svg",
  "size": { "width": 340, "height": 340 },
  "handles": [
    { "type": "target", "position": "left",  "id": "input"  },
    { "type": "source", "position": "right", "id": "output" }
  ],
  "action": {
    "url": "/finnhub/get-quote",
    "method": "POST"
  },
  "fields": [
    {
      "key": "credential_id",
      "label": "Credential",
      "type": "select",
      "optionsSource": "credentialsApi",
      "placeholder": "Select credential",
      "required": true
    },
    {
      "key": "ticker",
      "label": "Ticker Symbol",
      "type": "text",
      "placeholder": "e.g. AAPL",
      "required": true
    }
  ]
}
